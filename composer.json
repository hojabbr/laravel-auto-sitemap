{
    "name": "hojabbr/laravel-auto-sitemap",
    "description": "A dynamic sitemap generator for Laravel with support for multilingual content and model-based generation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojabbr",
            "email": "hojabbrdev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hojabbr\\LaravelAutoSitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hojabbr\\LaravelAutoSitemap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hojabbr\\LaravelAutoSitemap\\LaravelAutoSitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}